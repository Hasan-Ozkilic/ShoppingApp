@model List<FavoriGetAllModel>
@{
    ViewData["Title"] = "GetAll";
}

@section navbar{
    <partial name="_usernavbar" />
}

<h4 style="color:red;">Favorilerin</h4>

<div class="row mt-4">
    <partial name="_favoriyanmenu" />
    <div class="col-md-9">

        <partial name="_slider" />

        <div class="row mt-4">
            @foreach (var product in Model)
            {
                <partial name="_favori" model="product" />
            }
        </div>

    </div>
</div>


