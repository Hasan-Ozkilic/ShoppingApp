@model List<User>



<table class="table table-striped table-dark">
    <tr>
        <td>ID</td>
        <td>FirstName</td>
        <td>LastName</td>
        <td>Age</td>
        <td>E-Mail</td>
        <td>User Name</td>
        <td>Password</td>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Age</td>
            <td>@user.EMail</td>
            <td>@user.UserName</td>
            <td>@user.Password</td>
        </tr>
    }
</table>



